CREATE

CREATE TABLE Users (
useremail varchar2(20) PRIMARY KEY,
userpassword varchar2(20),
usermobile number unique,
userbalance number
)


CREATE TABLE ADMIN (
adminusername varchar2(20),
adminpassword varchar2(20)
)


CREATE TABLE BUS(
busid number PRIMARY KEY,
bustype varchar2(4),
bussource varchar2(15),
busdestination varchar2(15),
busfare number,
busseats number,
busdate date
);


SEQUENCE 
CREATE SEQUENCE busidSTART WITH 1 INCREMENT BY 1

INSERT

Insert into Admin values('admin','admin')


